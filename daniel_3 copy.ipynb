    "import folium\n",
    "from folium.plugins import TimestampedGeoJson\n",
    "import json\n",
    "\n",
    "# Centre de Paris (latitude et longitude)\n",
    "center_of_paris = [48.8566, 2.3522]\n",
    "\n",
    "# Taille du carré en degrés (environ, pour simplification)\n",
    "square_size = 0.01\n",
    "\n",
    "# Fonction pour créer un carré autour d'un point central\n",
    "def create_square(lat, lon, size):\n",
    "    return [\n",
    "        [lat - size, lon - size],\n",
    "        [lat - size, lon + size],\n",
    "        [lat + size, lon + size],\n",
    "        [lat + size, lon - size],\n",
    "        [lat - size, lon - size],  # Retour au point de départ pour fermer le polygone\n",
    "    ]\n",
    "\n",
    "# Création des polygones pour t1 (jaune) et t2 (rouge)\n",
    "square_t1 = create_square(center_of_paris[0], center_of_paris[1], square_size)\n",
    "square_t2 = create_square(center_of_paris[0], center_of_paris[1], square_size)\n",
    "\n",
    "# Données GeoJSON avec un timestamp pour chaque couleur\n",
    "data = {\n",
    "    \"type\": \"FeatureCollection\",\n",
    "    \"features\": [\n",
    "        {\n",
    "            \"type\": \"Feature\",\n",
    "            \"geometry\": {\n",
    "                \"type\": \"Polygon\",\n",
