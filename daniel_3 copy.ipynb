    "m = folium.Map(location=[48.815, 2.34], zoom_start=12)\n",
    "def generate_geojson_data(df):\n",
    "    for _, row in df.iterrows():\n",
    "        # Créer les limites du carré\n",
    "        size = 0.005  # Ajustez la taille des carrés\n",
    "        bounds = [\n",
    "            [row['lati'] - size / 2, row['long'] - size / 2],\n",
    "            [row['lati'] - size / 2, row['long'] + size / 2],\n",
    "            [row['lati'] + size / 2, row['long'] + size / 2],\n",
    "            [row['lati'] + size / 2, row['long'] - size / 2],\n",
    "            [row['lati'] - size / 2, row['long'] - size / 2]\n",
    "        ]\n",
    "        \n",
    "        # Retourner le GeoJSON pour chaque carré sans le stocker en mémoire\n",
    "        yield {\n",
    "            \"type\": \"Feature\",\n",
    "            \"geometry\": {\n",
    "                \"type\": \"Polygon\",\n",
    "                \"coordinates\": [bounds]\n",
    "            },\n",
    "            \"properties\": {\n",
    "                \"time\": row['t_1h'].isoformat(),\n",
    "                \"moyenne_k\": row['moyenne_k'],\n",
    "                \"style\": {\n",
    "                    \"color\": \"black\",  # Bordures noires\n",
    "                    \"fillColor\": f\"rgb({255-int(row['moyenne_k']*255/100)}, {int(row['moyenne_k']*255/100)}, 0)\",  # Échelle de couleurs\n",
    "                    \"fillOpacity\": 0.9\n",
    "                },\n",
    "                \"popup\": f\"<b>Carre ID:</b> {row['carre_id']}<br>\"\n",
    "                         f\"<b>Heure:</b> {row['t_1h']}<br>\"\n",
    "                         f\"<b>Moyenne K:</b> {row['moyenne_k']:.2f}\"\n",
    "            }\n",
    "        }\n",
    "\n",
    "# Utiliser le générateur pour ajouter les GeoJSONs à la carte\n",
    "geojson_data = list(generate_geojson_data(df))\n",
    "\n",
    "if geojson_data:\n",
    "    TimestampedGeoJson(\n",
    "        {\"type\": \"FeatureCollection\", \"features\": geojson_data},\n",
    "        period=\"PT1H\",  # Intervalle d'une heure\n",
    "        duration=\"PT1H\",  # Durée d'affichage d'une étape\n",
    "        auto_play=True,\n",
    "        loop=True,\n",
    "        add_last_point=True\n",
    "    ).add_to(m)\n",
    "else:\n",
    "    print(\"Erreur : Aucun GeoJSON n'a été généré. Vérifiez les données.\")\n"
   ]
  },
